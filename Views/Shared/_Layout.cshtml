<!DOCTYPE html>
<html lang="en">
<head>
            @{
                await  Html.RenderPartialAsync("_head");
            }
</head>
<body>
    <div class="wrapper">
        <header>
            @{
                await  Html.RenderPartialAsync("_header");
            }
        </header>

        <main role="main"  class="pb-3">
            @RenderBody()
        </main>

        <footer class="fixed-bottom">
            @{
                 await  Html.RenderPartialAsync("_footer");
            }
        </footer>

    </div>
</body>
</html>
